{
  "include": ["backend"],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.venv",
    "frontend"
  ],
  "venvPath": "./backend",
  "venv": ".venv",
  "pythonVersion": "3.9",
  "typeCheckingMode": "basic",
  "executionEnvironments": [
    {
      "root": "./backend",
      "extraPaths": ["./backend"]
    }
  ]
}

